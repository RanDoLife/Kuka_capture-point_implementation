<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">

  <xacro:include filename="$(find kuka_kr3_support)/urdf/kr.transmission.xacro" />
  <xacro:include filename="$(find kuka_kr3_support)/urdf/kr.gazebo.xacro" />

